            name4 = u.name;
            email4 = u.email;
        }
        if (u.uid.Equals(ViewBag.L5))
        {
            name5 = u.name;
            email5 = u.email;
        }
    }

    foreach (var course in ViewBag.CourseList)
    {
        n++;
    }

    foreach (var course in ViewBag.CourseList)
    {
        if (p <= 5)
        {
            CourseId[p] = course.coid;
            CourseTitle[p] = course.title;
            CoursePrice[p] = course.price;
            Details[p] = course.details;
        }

        p++;

