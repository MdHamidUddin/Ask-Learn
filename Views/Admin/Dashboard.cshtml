@model List<AskNLearn.Models.Entity.AdminDashboardModel>
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/MyLayout.cshtml";
}

<br>
@{
    int r = 0;
    int n = 0;
    int Instructor=0; int Learner=0; int Moderator=0;
    if(Model!=null)
    {

        foreach (var user in Model)
        {

            if (user.userType.Equals("Learner"))
            {
                Learner++;
            }
            else if (user.userType.Equals("Instructor"))
            {
                Instructor++;
            }
            else if (user.userType == "Moderator")
            {
                Moderator++;
            }
            r = user.reputation;
            n++;

        }
    }

    int TotalUser = Instructor + Learner + Moderator;

    double ins = ((double)Instructor / (double) TotalUser) * 100;
    double learner = ((double)Learner / (double)TotalUser) * 100;
    double moderator = ((double)Moderator/(double)TotalUser) * 100;

    /// Finding 3 largest number from User Info database
    /// 
    int large1 =Model[0].reputation;
    int large2 = 0;
    int large3 = 0;
    int totalReputation=0;
    int uid1 = Model[0].uid;
    int uid2 = 0;
    int uid3=0;
    string uname1 = Model[0].name; 
    string uname2 = "";
    string uname3="";
    string email1 = Model[0].email;
    string email2 = "";
    string email3="";
    string uType1=Model[0].userType;
    string uType2 = "";
    string uType3 = "";
    for (int i=0;i<n;i++)
    {
        if(Model[i].reputation>large1)
        {
            large3 = large2;
            large2 = large1;
            large1 = Model[i].reputation;
            uid1 = Model[i].uid;
            uname1 = Model[i].name;
            email1 = Model[i].email;
            uType1 = Model[i].userType;
        }
        else if(Model[i].reputation>large2)
        {
            large3 = large2;
            large2 = Model[i].reputation;
            uname3 = uname2;
            uType3 = uType2;
            email3 = email2;
            uid2 = Model[i].uid;
            uname2 = Model[i].name;
            email2 = Model[i].email;
            uType2= Model[i].userType;

        }

        else if(Model[i].reputation>large3)
        {
            large3 = Model[i].reputation;
            uid3 = Model[i].uid;
            uname3 = Model[i].name;
            email3 = Model[i].email;
            uType3= Model[i].userType;
        }
        totalReputation += Model[i].reputation;
    }
    // totalReputation = large1 + large2 + large3;




}



